<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Dashboard"
    android:orientation="vertical"
    android:background="@color/colorWhite">


    <FrameLayout
        android:id="@+id/frag_dashboard"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"/>

    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:id="@+id/meow_bottom_nav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorTransparent"
        app:mbn_backgroundBottomColor="@color/colorSlate"
        app:mbn_defaultIconColor="@color/colorWhite"
        app:mbn_circleColor="@color/colorSlate"
        app:mbn_selectedIconColor="@color/colorWhite"
        app:mbn_shadowColor="@color/colorMaroon"/>

</LinearLayout>